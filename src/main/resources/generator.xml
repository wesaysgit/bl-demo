			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 生成DAO的包名和位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.zld.dao.mapper" targetProject="/Users/xugan/bolink/generator/src">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 要生成那些表(更改tableName和domainObjectName就可以) -->
		<table tableName="union_operator_tb" domainObjectName="UnionOperatorTb"
